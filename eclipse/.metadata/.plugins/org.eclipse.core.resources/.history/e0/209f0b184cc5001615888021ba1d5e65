import numpy as np
from matplotlib import pyplot as pp

if __name__ == '__main__':
    
    SIN_FREQ = 16
    SIN_NUM_OF_PERS = 32
    SIN_SAMPLE_RATE = 32
    
    x = np.sin( 2*np.pi*SIN_FREQ/SIN_SAMPLE_RATE*np.arange( SIN_NUM_OF_PERS*SIN_SAMPLE_RATE ) ) 
    
    pyplot.figure()
    pyplot.plot( x )
    pyplot.show()
    pass